{
  "name": "@magic-sdk/provider",
  "version": "1.0.0-canary.0",
  "description": "Core business logic for Magic SDK packages.",
  "author": "Fortmatic <team@fortmatic.com> (https://fortmatic.com/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fortmatic/magic-js"
  },
  "homepage": "https://magic.link",
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "esmodule": "dist/index.modern.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "microbundle watch src/index.ts --target web --format modern,es,cjs --external none",
    "build": "microbundle build src/index.ts --target web --format modern,es,cjs --external none"
  },
  "dependencies": {
    "@magic-sdk/types": "1.0.0-canary.0",
    "eventemitter3": "^4.0.4"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.9.6"
  },
  "peerDependencies": {},
  "ava": {
    "require": [
      "ts-node/register",
      "./test/setup.ts"
    ],
    "files": [
      "test/**/*.spec.ts"
    ],
    "extensions": [
      "ts",
      "tsx"
    ],
    "compileEnhancements": false,
    "verbose": true
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": false,
    "check-coverage": true,
    "per-file": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "reporter": [
      "html",
      "lcov"
    ]
  }
}
